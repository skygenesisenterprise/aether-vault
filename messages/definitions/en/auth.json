  "VAULT_AUDIT_ACCESS_LOG": {
    "id": "VAULT_AUDIT_ACCESS_LOG",
    "domain": "audit",
    "type": "audit",
    "severity": "info",
    "template": "User '{user_id}' accessed resource '{resource}' via {action} at {timestamp}.",
    "variables": ["user_id", "resource", "action", "timestamp"],
    "context": {
      "audit_type": "access",
      "error_code": "AUDIT_001"
    },
    "metadata": {
      "added_version": "1.0.0",
      "deprecated": false,
      "security_sensitive": true,
      "description": "Audit log for resource access"
    }
  },
  "VAULT_AUDIT_SECURITY_EVENT": {
    "id": "VAULT_AUDIT_SECURITY_EVENT",
    "domain": "audit",
    "type": "audit",
    "severity": "warning",
    "template": "Security event detected: {event_type} by user '{user_id}' from {ip_address} at {timestamp}.",
    "variables": ["event_type", "user_id", "ip_address", "timestamp"],
    "context": {
      "audit_type": "security",
      "error_code": "AUDIT_002"
    },
    "metadata": {
      "added_version": "1.0.0",
      "deprecated": false,
      "security_sensitive": true,
