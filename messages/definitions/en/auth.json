{
  "VAULT_AUTH_INVALID_TOKEN": {
    "id": "VAULT_AUTH_INVALID_TOKEN",
    "domain": "auth",
    "type": "user",
    "severity": "error",
    "template": "Invalid authentication token. Please provide a valid token.",
    "variables": ["token"],
    "context": {
      "http_status": 401,
      "error_code": "AUTH_001"
    },
    "metadata": {
      "added_version": "1.0.0",
      "deprecated": false,
      "security_sensitive": true,
      "description": "Returned when authentication token is invalid or expired"
    }
  },
  "VAULT_AUTH_MISSING_TOKEN": {
    "id": "VAULT_AUTH_MISSING_TOKEN",
    "domain": "auth",
    "type": "user",
    "severity": "error",
    "template": "Authentication token is required. Please provide a valid token.",
    "variables": [],
    "context": {
      "http_status": 401,
      "error_code": "AUTH_002"
    },
    "metadata": {
      "added_version": "1.0.0",
      "deprecated": false,
      "security_sensitive": false,
      "description": "Returned when no authentication token is provided"
    }
  },
  "VAULT_AUTH_EXPIRED_TOKEN": {
    "id": "VAULT_AUTH_EXPIRED_TOKEN",
    "domain": "auth",
    "type": "user",
    "severity": "error",
    "template": "Authentication token has expired. Please refresh your token.",
    "variables": ["token"],
    "context": {
      "http_status": 401,
      "error_code": "AUTH_003"
    },
    "metadata": {
      "added_version": "1.0.0",
      "deprecated": false,
      "security_sensitive": true,
      "description": "Returned when authentication token has expired"
    }
  },
  "VAULT_SECRET_NOT_FOUND": {
