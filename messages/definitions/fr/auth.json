{
  "VAULT_AUTH_INVALID_TOKEN": {
    "id": "VAULT_AUTH_INVALID_TOKEN",
    "domain": "auth",
    "type": "user",
    "severity": "error",
    "template": "Token d'authentification invalide. Veuillez fournir un token valide.",
    "variables": ["token"],
    "context": {
      "http_status": 401,
      "error_code": "AUTH_001"
    },
    "metadata": {
      "added_version": "1.0.0",
      "deprecated": false,
      "security_sensitive": true,
      "description": "Retourné lorsque le token d'authentification est invalide ou expiré"
    }
  },
