{
  "name": "aether-vault",
  "package": "@aether-vault/vscode",
  "displayName": "Aether Vault Vscode",
  "description": "VS Code extension for Aether Vault - Unified secrets and TOTP management",
  "version": "1.0.0",
  "publisher": "skygenesisenterprise",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vault",
    "totp",
    "aether-vault",
    "vscode",
    "extension",
    "secrets"
  ],
  "author": "Sky Genesis Enterprise",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skygenesisenterprise/aether-vault.git",
    "directory": "package/vscode"
  },
  "homepage": "https://github.com/skygenesisenterprise/aether-vault",
  "bugs": {
    "url": "https://github.com/skygenesisenterprise/aether-vault/issues"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aether-vault.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "activationEvents": [
    "onCommand:aether-vault.helloWorld"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^5.9.3"
  }
}
